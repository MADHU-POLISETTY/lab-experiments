#include <stdio.h>

int main() {
    int b[10], p[10], nb, np, allocation[10];
    int i, j;

    scanf("%d%d", &nb, &np);
    for (i = 0; i < nb; i++) scanf("%d", &b[i]);
    for (i = 0; i < np; i++) scanf("%d", &p[i]);

    for (i = 0; i < np; i++) {
        int best = -1;
        for (j = 0; j < nb; j++)
            if (b[j] >= p[i] && (best == -1 || b[j] < b[best]))
                best = j;

        if (best != -1) {
            allocation[i] = best;
            b[best] -= p[i];
        } else allocation[i] = -1;
    }

    for (i = 0; i < np; i++)
        printf("P%d -> B%d\n", i + 1, allocation[i] + 1);

    return 0;
}
