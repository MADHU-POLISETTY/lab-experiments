#include <stdio.h>
#include <fcntl.h>
#include <unistd.h>

int main() {
    int fd = open("test.txt", O_CREAT | O_WRONLY, 0644);
    write(fd, "Hello UNIX\n", 11);
    close(fd);

    char buf[50];
    fd = open("test.txt", O_RDONLY);
    read(fd, buf, sizeof(buf));
    printf("%s", buf);
    close(fd);
    return 0;
}
